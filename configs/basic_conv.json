{
  "exp": {
    "name": "basic_conv"
  },
  "data_loader": {
    "name": "data_loader.DataLoader"
  },
  "model": {
    "name": "model.ConvolutionModel",
    "learning_rate": 0.001,
    "optimizer": "adam"
  },
  "trainer": {
    "name": "trainer.ModelTrainer",
    "num_epochs": 25
  },
  "callbacks": {
    "checkpoint_monitor": "val_loss",
    "checkpoint_mode": "min",
    "checkpoint_save_best_only": true,
    "checkpoint_save_weights_only": true,
    "checkpoint_verbose": true,
    "tensorboard_write_graph": true
  },
  "image_size": {
      "x": 180,
      "y": 180 
  },
  "batch_size": 32
}
